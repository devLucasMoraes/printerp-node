{
  "name": "printerp-backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "pnpm docker:dev && pnpm migration:run && tsx watch src/server.ts",
    "dev:local": "pnpm migration:run && tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "start:prod:win": "set NODE_ENV=production&& pnpm migration:run&& node dist/server.js",
    "test": "NODE_ENV=test pnpm docker:test && pnpm migration:run && jest --runInBand",
    "posttest": "pnpm docker:test:down",
    "test:watch": "jest --watchAll --runInBand --verbose",
    "typeorm": "tsx ./node_modules/typeorm/cli.js -d src/database/index.ts",
    "migration:generate": "pnpm typeorm migration:generate src/database/migrations/default",
    "migration:run": "pnpm typeorm migration:run",
    "docker:dev": "docker compose -f infra/compose.dev.yaml up -d",
    "docker:dev:down": "docker compose -f infra/compose.dev.yaml down",
    "docker:test": "docker compose -f infra/compose.test.yaml up -d",
    "docker:test:down": "docker compose -f infra/compose.test.yaml down",
    "docker:prod": "docker compose -f infra/compose.prod.yaml up -d",
    "docker:prod:down": "docker compose -f infra/compose.prod.yaml down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async-retry": "^1.3.3",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^8.0.0",
    "http-graceful-shutdown": "^3.1.13",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.6.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.9",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
